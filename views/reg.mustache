<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <div class="regForm">
      <legend>Register</legend>
      <fieldset>
        <form class="" action="/register" method="post">
          <input type="text" name="regusername" value="Username" onblur="if (this.value == '') {this.value = 'Username';}" onfocus="if (this.value == 'Username') {this.value = '';}">
          <input type="repassword" name="regpassword" value="Password" onblur="if (this.value == '') {this.value = 'Password';}" onfocus="if (this.value == 'Password') {this.value = '';}">
          <input type="password" name="conpassword" value="Password" onblur="if (this.value == '') {this.value = 'Password';}" onfocus="if (this.value == 'Password') {this.value = '';}">
          <input type="submit" name="" value="Submit">
        </form>
      </fieldset>
    </div>
  </body>
</html>
